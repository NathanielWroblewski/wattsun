$(document).ready(function(){
  if ($('#map').length > 0) {
    jQuery.support.cors = true;

    $.ajax('http://www.zillow.com/webservice/GetSearchResults.htm' +
      '?zws-id=X1-ZWz1bdsmwbv7yj_963mm' +
      '&address=' + $('#map').data('address') +
      '&citystatezip=' + $('#map').data('zip') + '', {
      dataType: "JSON",
      complete: function(result, success){
        if (success === 'error') {
          console.log('loser');
        }
        else {
          console.log('winner');
        }
      }
    })
  }
});
