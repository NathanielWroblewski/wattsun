<% if @address && @zip %>
  $(document).ready(function(){
    window.address = <%= @address %>
    window.zip = <%= @zip %>
  });
<% end %>

$(document).ready(function(){
  if (typeof window.address != 'undefined') {
    $.ajax('http://www.zillow.com/webservice/GetSearchResults.htm' +
      '?zws-id=X1-ZWz1bdsmwbv7yj_963mm' +
      '&address=' + window.address +
      '&citystatezip=' + window.zip, function(data){
      console.log(data)
    });
  }
});
